<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="600.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.gcmsystem.gcmsystemdesktop.controller.HomeController">
   <left>
      <VBox alignment="CENTER" prefHeight="600.0" prefWidth="200.0" BorderPane.alignment="BOTTOM_LEFT">
         <children>
            <Button fx:id="equipamentoButton" focusTraversable="false" mnemonicParsing="false" prefHeight="37.0" prefWidth="125.0" text="Equipamento" textAlignment="CENTER">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Button>
            <Button fx:id="gcmButton" focusTraversable="false" mnemonicParsing="false" prefHeight="37.0" prefWidth="125.0" text="GCM">
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="registroButton" focusTraversable="false" mnemonicParsing="false" prefHeight="37.0" prefWidth="125.0" text="Cautela">
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </left>
</BorderPane>
